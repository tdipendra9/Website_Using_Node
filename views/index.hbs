<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css ">
    <link rel="stylesheet" href="/static/css/style.css">
    <title>Dynamic Website Using Node JS</title>
  </head>
  <body>
    {{!-- navbar section --}}
    {{>navbar data=details}}

    {{!-- slider section  --}}
    {{>slider slides=slides}}

    {{!-- banner1 section  --}}
    <section class="banner1_section background1 py-5">
      <div class="container text-center ">
        <div class="row">
          <div class="col-md-12">
            <h1>Start Learning new Technology here !!</h1>
            <p>
              Lorem ipsum dolor, sit amet consectetur adipisicing elit. Praesentium, sint?
            </p>
            <button class=" btn btn-outline-primary">
              Start Learning
            </button>
          </div>
        </div>
      </div>
    </section>

    {{!-- services section --}}
     <section id="services_section" class="py-4">

      <div class="container">
        <h1 class="text-center text-secondary ">Services that we Provide</h1>

      <div class="row mt-5">
        {{!-- first service column --}}
        {{#each services }}
        <div class="col-md-4">
        <div class="card">
          <div class="card-body">
           <div class="container text-center py-3 ">
            <i class="{{this.icon}} service_icon text-secondary "></i>
           </div>
            <h5 class="card-title">
              {{this.title}}
            </h5>
            <p>
              {{this.description }}
            </p>
              <a href="{{this.link}}">{{this.linkText}}</a>
              <a href="#!" class="ml-3">Support</a>

          </div>
           
        </div>

        </div>
        {{/each}}
      </div>

      
      </div>

      {{!-- Banner  two Section Here --}}

        <section class="banner2_section py-5 ">
          <div class="container">
            <div class="row">
              <div class="col-md-6 text-center pt-5">
                {{!-- left side  --}}
                 <h3>We ork for people, not for money  </h3>
                 <p>
                  Lorem ipsum dolor sit, amet consectetur adipisicing elit. 
                  Error accusamus quia rem! Sint unde quam earum, voluptate, architecto sed 
                  vitae pariatur nostrum iure fugit vel fuga illum odio, aliquid ipsum.
                 </p>
                 <button class="btn btn-primary">
                  Click Here 
                 </button>
              </div>
              <div class="col-md-6 text-center pt-3">
                {{!-- Right side  --}}
                <img src="/static/images/one.svg" alt="svg image alternate" style="max-height:350px;">
              </div>
            </div>
          </div>
        </section>

      </section>

           {{!-- Banner  three Section Here --}}
            <section class="banner2_section py-5 ">
          <div class="container">
            <div class="row">
               <div class="col-md-6 text-center pt-3">
                {{!-- Left side  --}}
                <img src="/static/images/two.svg" class="img-fluid" alt="svg image alternate" style="max-height:350px;">
              </div>

              <div class="col-md-6 text-center pt-5">
                {{!-- Right side  --}}
                 <h3>We ork for people, not for money  </h3>
                 <p>
                  Lorem ipsum dolor sit, amet consectetur adipisicing elit. 
                  Error accusamus quia rem! Sint unde quam earum, voluptate, architecto sed 
                  vitae pariatur nostrum iure fugit vel fuga illum odio, aliquid ipsum.
                 </p>
                 <button class="btn btn-primary">
                  Click Here 
                 </button>
              </div>
             
            </div>
          </div>
        </section>

      </section>


      {{!-- contact us section --}}
       <section id="contact_us_section" class="py-5 background1">
        <div class="container">
          <h3 class="text-center text-secondary">Contact us</h3>
          <div class="row">
            <div class="col-md-12">
              
              <form action="/process-contact-form" method="post">

  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Your email address</label>
    <input name="email" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
  </div>


  <div class="mb-3">
    <label for="contactNumberField" class="form-label">Your contact number</label>
    <input name="phone" type="number" class="form-control" id="contactNumberField">
  </div>

<div class="form-floating my-2">
  <textarea name="query" style="height:250px; " class="form-control" placeholder="Enter your query here" id="queryField"></textarea>
  <label for="queryField">Comments</label>
</div>

  <div class="container text-center">
     
  <button type="submit" class="btn btn-dark">Submit</button>
  </div>



</form>
            </div>
          </div>
        </div>

       </section>

       {{!-- About Section Here LAST --}}
     <section class="about_section py-5" id="about_section">
      <div class="container text-center ">
        <div class="row">
          <div class="col-md-12">
